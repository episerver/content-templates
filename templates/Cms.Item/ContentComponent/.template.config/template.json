{
  "$schema": "http://json.schemastore.org/template",
  "author": "Episerver AB",
  "sourceName": "MyContentComponent",
  "classifications": [ "Web" ],
  "identity": "EPiServer.Templates.Cms.Item.ContentComponent",
  "name": "Optimizely CMS Content Type Model",
